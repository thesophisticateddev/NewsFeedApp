<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.geometry.Insets?>
<VBox fx:id="liveUpdates"  xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="com.example.demo.controllers.LiveUpdatesController">
    <AnchorPane maxWidth="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="691.0" prefWidth="996.0"
                VBox.vgrow="ALWAYS">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
        <TextArea fx:id="loggerTxtArea" editable="false" prefHeight="484.0" prefWidth="996.0" wrapText="true"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0"/>
    </AnchorPane>
    <AnchorPane prefHeight="31.0" prefWidth="1086.0">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
        <Button mnemonicParsing="false" text="Clear Log" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"/>
        <Button mnemonicParsing="false" text="Print Log" AnchorPane.bottomAnchor="0.0"
                AnchorPane.rightAnchor="0.0"/>
    </AnchorPane>
</VBox>
